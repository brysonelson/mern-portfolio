{
  "name": "portfolio",
  "version": "1.0.0",
  "engines": {
    "node": "6.11.1"
  },
  "description": "",
  "main": "server.js",
  "license": "MIT",
  "repository": "https://github.com/brysonelson/mern-portfolio.git",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "seed": "node scripts/seedDB.js",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "author": "Bryson Nelson",
  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.7"
  },
  "dependencies": {
    "@sendgrid/mail": "^6.3.1",
    "animate.css": "^3.7.0",
    "axios": "^0.18.0",
    "dotenv": "^6.2.0",
    "express": "^4.16.3",
    "if-env": "^1.0.4",
    "jquery": "^3.3.1",
    "mongoose": "^5.3.16",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-ga": "^2.5.7",
    "react-inview-monitor": "^2.2.1",
    "react-masonry-component": "^6.2.1",
    "react-materialize": "^2.6.0",
    "sweetalert2": "^8.3.0"
  }
}
